{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "bulkinout",
            "runtimeArgs": [
                "--preserve-symlinks"
            ],
            "program": "${workspaceFolder}/index.js",
            "env": {
                "DOCTOR_ACCOUNT": "stg-thanos",
                "ELEMENT_TOKEN": "",
                "ELEMENT_KEY":"smartrecruiters",
                "ELEMENT_RESOURCE":"jobs"
                //"BASE_URL": "https://staging.cloud-elements.com",
                //"USER_SECRET": "",
                //"ORG_SECRET": "",
            },
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
                "${workspaceFolder}/lib/**/*.js",
                "<node_internals>/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "vendor-smartrecruiters",
            "runtimeArgs": [
                "--preserve-symlinks"
            ],
            "program": "${workspaceFolder}/index.js",
            "env": {
                "VENDOR_DIRECT": "true",
                "ELEMENT_TOKEN": "",
                "ELEMENT_KEY":"smartrecruiters",
                "ELEMENT_RESOURCE":"jobs",
                "BASE_URL": "https://api.smartrecruiters.com"
            },
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
                "${workspaceFolder}/lib/**/*.js",
                "<node_internals>/**/*.js"
            ]
        }
    ]
}